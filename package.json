{
  "name": "frakt-web",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^5.7.0",
    "@frakters/frkt-staking-library": "^1.0.6",
    "@solana/spl-token-registry": "^0.2.64",
    "@solana/wallet-adapter-base": "^0.7.1",
    "@solana/wallet-adapter-react": "^0.13.1",
    "@solana/wallet-adapter-wallets": "^0.11.3",
    "@solana/web3.js": "^1.31.0",
    "antd": "^4.6.6",
    "bn.js": "^5.1.3",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "frakt-client": "^1.5.9",
    "moment": "^2.29.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "^4.0.3",
    "sass": "^1.43.4",
    "solana-nft-metadata": "^1.0.3",
    "swiper": "^7.4.1"
  },
  "scripts": {
    "start": "npm run devnet",
    "devnet": "cross-env REACT_APP_NETWORK=devnet craco start",
    "mainnet": "craco start",
    "build": "craco build",
    "build:devnet": "cross-env REACT_APP_NETWORK=devnet craco build",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier-eslint --write $PWD/'**/*.{ts,tsx}'",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frakt-solana/frakt-web"
  },
  "devDependencies": {
    "@types/bn.js": "^4.11.6",
    "@types/bs58": "^4.0.1",
    "@types/jest": "^24.9.1",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.2",
    "@types/react-router-dom": "^5.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "typescript": "^4.4.3"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.1.1",
    "**/@typescript-eslint/parser": "^4.1.1"
  }
}
